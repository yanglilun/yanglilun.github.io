  <p class=MsoNormal><span lang=EN-US>}</span></p>
  <p class=MsoNormal><span lang=EN-US>char* <span class=GramE>jm(</span>char
  text[])</span></p>
  <p class=MsoNormal><span lang=EN-US>{</span></p>
